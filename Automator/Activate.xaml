<Window x:Class="Automator.Activate"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Activate" Height="300" Width="300">
    <StackPanel>
        <Label>Enter the activation code</Label>
        <TextBox FontFamily="courier" Name="Code"></TextBox>
        <Button Click="Activation">Activate</Button>
    </StackPanel>
</Window>
